    "description": "Simplify Symfony2 RESTful API building",
    "keywords": ["rest", "api", "restful api", "resourceful controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Fortunato",
            "email": "jfortunato25@gmail.com"
        }
    ],
    "require": {
        "friendsofsymfony/rest-bundle": "~1.4",
        "jms/serializer-bundle": "0.12.*",
        "symfony/form": "*",
    },
    "require-dev": {
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Fortune\\ResourceBundle\\": "",
            "Fortune\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "nelmio/api-doc-bundle": "Free documentation for your API"
    }
}
